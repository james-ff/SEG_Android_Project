<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Values for LogoTextView, for info on how to add more
    	 logo types see LogoTextView.java -->
    <declare-styleable name="LogoTextView">
	    <attr name="logo_type" format="integer" >
	        <enum name="home" value="0" />
	        <enum name="briefcase" value="1" />  
	        <enum name="road" value="2" />
	        <enum name="tags" value="3" />
	        <enum name="arrow_left" value="4" />
	        <enum name="arrow_right" value="5" />
	    </attr>
    </declare-styleable>
</resources>
